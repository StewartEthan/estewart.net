{
  "blocks": false,
  "brackets": "never",
  "colons": "never",
  "colors": false,
  "commaSpace": false,
  "commentSpace": "always",
  "depthLimit": false,
  "duplicates": true,
  "efficient": "always",
  "extendPref": "@extend",
  "globalDupe": false,
  "groupOutputByFile": true,
  "indentPref": 2,
  "leadingZero": false,
  "mixed": true,
  "namingConvention": "lowercase-dash",
  "namingConventionStrict": false,
  "none": false,
  "noImportant": true,
  "parenSpace": false,
  "placeholder": "always",
  "prefixVarsWithDollar": "always",
  "quotePref": "single",
  "reporter": "stylint-stylish",
  "semicolons": "never",
  "sortOrder": false,
  "stackedProperties": "never",
  "valid": true,
  "zeroUnits": "never"
}